<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/information_background" />

    <EditText
        android:id="@+id/editTextDosis"
        android:layout_width="203dp"
        android:layout_height="76dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Dosis"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextMedicamentos" />

    <EditText
        android:id="@+id/editTextHorario"
        android:layout_width="203dp"
        android:layout_height="76dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Horario"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextVia" />

    <EditText
        android:id="@+id/editTextVia"
        android:layout_width="203dp"
        android:layout_height="76dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Vía"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDosis" />

    <EditText
        android:id="@+id/editTextMedicamentos"
        android:layout_width="203dp"
        android:layout_height="131dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Medicamentos"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextCama" />

    <EditText
        android:id="@+id/editTextCama"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Cama"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextFechaI" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="199dp"
        android:layout_height="60dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/history" />

    <EditText
        android:id="@+id/editTextFechaI"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/edittext_background"
        android:drawablePadding="16dp"
        android:ems="10"
        android:hint="Fecha de ingreso"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#3B608c"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="92dp"
        android:layout_height="36dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="25dp"
        android:text="Fecha ingreso:"
        app:layout_constraintEnd_toStartOf="@+id/editTextMedicamentos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="25dp"
        android:text="Cama:"
        app:layout_constraintEnd_toStartOf="@+id/editTextMedicamentos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="108dp"
        android:layout_height="38dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="30dp"
        android:text="Medicamento(s):"
        app:layout_constraintEnd_toStartOf="@+id/editTextMedicamentos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="120dp"
        android:text="Dosis:"
        app:layout_constraintEnd_toStartOf="@+id/editTextDosis"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="90dp"
        android:text="Vía:"
        app:layout_constraintEnd_toStartOf="@+id/editTextVia"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="75dp"
        android:text="Horario:"
        app:layout_constraintEnd_toStartOf="@+id/editTextHorario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

</androidx.constraintlayout.widget.ConstraintLayout>